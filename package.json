{
  "name": "todo",
  "version": "1.0.0",
  "description": "",
  "author": "Jason Etcovitch <jasonetco@gmail.com> (https://github.com/jasonetco/todo)",
  "license": "ISC",
  "repository": "https://github.com/JasonEtco/todo.git",
  "scripts": {
    "start": "sls offline",
    "serve": "probot run ./index.js",
    "deploy:dev": "sls deploy --stage dev",
    "deploy:prod": "sls deploy --stage production",
    "test": "jest --coverage && standard",
    "postinstall": "cp -R node_modules/probot probot"
  },
  "standard": {
    "env": {
      "jest": true
    }
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/__tests__/fixtures/",
      "<rootDir>/__tests__/helpers.js",
      "<rootDir>/__tests__/setup.js"
    ],
    "setupFiles": [
      "<rootDir>/__tests__/setup.js"
    ],
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": true
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "file-loader": "^1.1.5",
    "github-webhook-handler": "^0.7.1",
    "probot": "^4.1.0",
    "serverless-offline": "^3.16.0",
    "serverless-webpack": "^4.2.0",
    "standard": "^10.0.3",
    "webpack": "^3.6.0",
    "webpack-node-externals": "^1.6.0"
  },
  "engines": {
    "node": ">= 8.6.0",
    "npm": ">= 5.0.0"
  },
  "devDependencies": {
    "jest": "^22.0.6",
    "localtunnel": "^1.8.2",
    "supertest": "^3.0.0"
  }
}
