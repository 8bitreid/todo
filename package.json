{
  "name": "todo",
  "version": "1.0.0",
  "description": "",
  "author": "Jason Etcovitch <jasonetco@gmail.com> (https://github.com/jasonetco/todo)",
  "license": "ISC",
  "repository": "https://github.com/JasonEtco/todo.git",
  "scripts": {
    "start": "nodemon --exec \"probot\" run ./index.js",
    "webpack": "webpack -w --config ./config/webpack.config.js",
    "build": "rimraf public && webpack -p --config ./config/webpack.config.js --progress --profile --colors",
    "serve": "probot run ./index.js",
    "test": "jest && codecov && standard"
  },
  "standard": {
    "env": {
      "jest": true
    }
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/__tests__/fixtures/"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "dependencies": {
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "codemirror": "^5.30.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "primer-css": "^9.4.0",
    "probot": "probot/probot",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "react": "^16.0.0",
    "react-codemirror": "^1.0.0",
    "react-dom": "^16.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0"
  },
  "devDependencies": {
    "codecov": "^2.3.0",
    "jest": "^21.2.1",
    "localtunnel": "^1.8.2",
    "nodemon": "^1.12.1",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": ">= 8.6.0",
    "npm": ">= 5.0.0"
  }
}
