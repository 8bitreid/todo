{
  "name": "todo",
  "version": "1.0.0",
  "description": "",
  "author": "Jason Etcovitch <jasonetco@gmail.com> (https://github.com/jasonetco/todo)",
  "license": "ISC",
  "repository": "https://github.com/JasonEtco/todo.git",
  "scripts": {
    "starts": "probot run ./index.js",
    "test": "jest --coverage && standard",
    "test:update": "jest -u"
  },
  "standard": {
    "env": {
      "jest": true
    }
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/__tests__/fixtures/",
      "<rootDir>/__tests__/helpers.js",
      "<rootDir>/__tests__/setup.js"
    ],
    "setupFiles": [
      "<rootDir>/__tests__/setup.js"
    ],
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": true
  },
  "dependencies": {
    "probot": "^3.0.0"
  },
  "devDependencies": {
    "jest": "^21.2.1",
    "localtunnel": "^1.8.2",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": ">= 8.6.0",
    "npm": ">= 5.0.0"
  }
}
